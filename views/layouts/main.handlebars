            <!-- Dropdown menu links -->
            <div class="container">
              <form action="" method="post">
                <label for="email">Email:</label>
                <input type="text" placeholder="Email" />
                <label for="Password">Password:</label>
                <input type="password" placeholder="Password" />
              </form>
